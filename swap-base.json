{
  "description": "Switch to # (Swap Slot)",
  "manipulators": [
    {
      "from": {
        "key_code": "#",
        "modifiers": {
          "mandatory": [
            "shift",
            "command"
          ]
        }
      },
      "to": [
        {
          "key_code": "#",
          "modifiers": [
            "control"
          ]
        },
        {
          "shell_command": "open -a #"
        }
      ],
      "type": "basic"
    }
  ]
}
