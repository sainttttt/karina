[
  {
    "description": "rename tmux pane",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": "##editorsandterms",
            "type": "frontmost_application_if"
          },
          {
            "type": "variable_if",
            "name": "nav_layer",
            "value": 1
          }
        ],

        "from": {
          "key_code": "e",
          "modifiers": {
            "mandatory": [
              "command"
            ]
          }
        },

        "to": [
          {
            "key_code": "b",
            "modifiers": [
              "control"
            ]
          },
          {
            "key_code": "comma"
          }
        ],
        "type": "basic"
      }
    ]
  },

