[

  {
    "description": "command q to command n new",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": "##browsers",
            "type": "frontmost_application_if"
          }
        ],
        "from": {
          "key_code": "q",
          "modifiers": {
            "mandatory": [
              "command"
            ]
          }
        },
        "to": [
          {
            "key_code": "n",
            "modifiers": [
              "command"
            ]
          }
        ],
        "type": "basic"
      }

    ]
  },

  { "description": "fromD Use Command+D to focus address bar in Firefox",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": [
              "^org.mozilla.*"
            ],
            "type": "frontmost_application_if"
          },
          {
            "type": "variable_if",
            "name": "nav_layer",
            "value": 0
          }
        ],
        "from": {
          "key_code": "d",
          "modifiers": {
            "mandatory": [
              "left_gui"
            ]
          }
        },
        "to": [
          {
            "key_code": "l",
            "modifiers": [
              "left_gui"
            ]
          }
        ],
        "type": "basic"
      }
    ]
  },

