  {
    "description": "command q to command n new",
    "manipulators": [
      {

        "conditions": [
          {
            "bundle_identifiers": "##browsers",
            "type": "frontmost_application_if"
          }
        ],
        "from": {
          "key_code": "q",
          "modifiers": {
            "mandatory": [
              "command"
            ]
          }
        },
        "to": [
          {
            "key_code": "n",
            "modifiers": [
              "command"
            ]
          }
        ],
        "type": "basic"
      }

    ]
  },

