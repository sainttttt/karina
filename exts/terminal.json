[
  {
    "description": "command i to option shift a for vim stuff",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": "##editorsandterms",
            "type": "frontmost_application_if"
          }
        ],

        "from": {
          "key_code": "i",
          "modifiers": {
            "mandatory": [
              "command"
            ]
          }
        },

        "to": {
          "key_code": "a",
          "modifiers": [
            "shift",
            "option"
          ]

        },
        "type": "basic"
      }
    ]
  },


{
    "description": "Change right_shift to sticky modifier",
    "manipulators": [
        {
            "type": "basic",
            "from": {
                "key_code": "grave_accent_and_tilde"
            },
            "to": [{ "key_code": "right_shift" }],
            "to_if_alone": [{ "sticky_modifier": { "right_shift": "toggle" } }]
        }
    ]
},

