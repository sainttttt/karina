[
  {
    "description": "fromS Finder disable",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": [
              "^com.apple.finder$"
            ],
            "type": "frontmost_application_if"
          }
        ],
        "from": {
          "key_code": "s",
          "modifiers": {
            "mandatory": [
              "option"
            ]
          }
        },
        "to": [
          {
            "key_code": "vk_none"
          }
        ],
        "type": "basic"
      }
    ]
  },

