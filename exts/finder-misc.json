            ]
          }
        },
        "to": [
          {
            "key_code": "g",
            "modifiers": [
              "command",
              "shift"
            ]
          },

          {
            "key_code": "vk_none",
            "hold_down_milliseconds": 100
          },
          {"shell_command":"osascript -e 'set temp to the clipboard \n set the clipboard to \"~/Screenshots/\" \n tell application \"System Events\" \n   keystroke \"v\" using command down \n   delay 0.15 \n end tell \n set the clipboard to temp'"},

          {
            "key_code": "vk_none",
            "hold_down_milliseconds": 400
          },

          {
            "key_code": "return_or_enter"
          }

        ],
        "type": "basic"
      }
    ]
  },

